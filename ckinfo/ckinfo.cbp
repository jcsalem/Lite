<?xml version="1.0" encoding="UTF-8" standalone="yes" ?>
<CodeBlocks_project_file>
	<FileVersion major="1" minor="6" />
	<Project>
		<Option title="ckinfo" />
		<Option pch_mode="2" />
		<Option compiler="gcc" />
		<Build>
			<Target title="Debug">
				<Option output="bin\Debug\ckinfo" prefix_auto="1" extension_auto="1" />
				<Option object_output="obj\Debug\" />
				<Option external_deps="..\cklib\bin\Debug\libcklib.a;" />
				<Option type="1" />
				<Option compiler="gcc" />
				<Compiler>
					<Add option="-g" />
				</Compiler>
			</Target>
			<Target title="Release">
				<Option output="bin\Release\ckinfo" prefix_auto="1" extension_auto="1" />
				<Option object_output="obj\Release\" />
				<Option external_deps="..\cklib\bin\Release\libcklib.a;" />
				<Option type="1" />
				<Option compiler="gcc" />
				<Compiler>
					<Add option="-O2" />
				</Compiler>
				<Linker>
					<Add option="-s" />
				</Linker>
			</Target>
		</Build>
		<Compiler>
			<Add option="-Wall" />
			<Add option="-fexceptions" />
		</Compiler>
		<Linker>
			<Add option="..\cklib\bin\$(TARGET_NAME)\libcklib.a -static-libgcc -static-libstdc++ -lws2_32" />
		</Linker>
		<ExtraCommands>
			<Add after="cp bin/$(TARGET_NAME)/$(TARGET_OUTPUT_BASENAME).exe ../bin/$(TARGET_OUTPUT_BASENAME).exe" />
		</ExtraCommands>
		<Unit filename="ckinfo.cpp" />
		<Extensions>
			<code_completion />
			<envvars />
			<debugger />
			<lib_finder disable_auto="1" />
		</Extensions>
	</Project>
</CodeBlocks_project_file>
