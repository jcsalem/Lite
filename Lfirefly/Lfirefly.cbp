<?xml version="1.0" encoding="UTF-8" standalone="yes" ?>
<CodeBlocks_project_file>
	<FileVersion major="1" minor="6" />
	<Project>
		<Option title="Lfirefly" />
		<Option pch_mode="2" />
		<Option compiler="gcc" />
		<Build>
			<Target title="Debug">
				<Option output="bin/Debug/Lfirefly" prefix_auto="1" extension_auto="1" />
				<Option object_output="obj/Debug/" />
				<Option external_deps="../lib/bin/Debug/liblite.a;" />
				<Option type="1" />
				<Option compiler="gcc" />
				<Option parameters="--pds 172.24.22.51@1r --pds 172.24.22.51@2" />
				<Compiler>
					<Add option="-g" />
				</Compiler>
			</Target>
			<Target title="Release">
				<Option output="bin/Release/Lfirefly" prefix_auto="1" extension_auto="1" />
				<Option object_output="obj/Release/" />
				<Option external_deps="../lib/bin/Release/liblite.a;" />
				<Option type="1" />
				<Option compiler="gcc" />
				<Compiler>
					<Add option="-O2" />
				</Compiler>
				<Linker>
					<Add option="-s" />
				</Linker>
			</Target>
		</Build>
		<Compiler>
			<Add option="-Wall" />
			<Add directory="../lib" />
		</Compiler>
		<Linker>
			<Add option="../lib/bin/$(TARGET_NAME)/liblite.a" />
			<Add option="$(#extralibs)" />
			<Add option="-static-libgcc -static-libstdc++" />
			<Add library="sfml-graphics$(#sfmlswitch)" />
			<Add library="sfml-window$(#sfmlswitch)" />
			<Add library="sfml-system$(#sfmlswitch)" />
			<Add directory="../../SFML-2.1/lib" />
		</Linker>
		<ExtraCommands>
			<Add after="cp bin/$(TARGET_NAME)/$(TARGET_OUTPUT_BASENAME)$(#FILEXT) ../bin/$(TARGET_OUTPUT_BASENAME)$(#FILEXT)" />
		</ExtraCommands>
		<Unit filename="Lfirefly.cpp" />
		<Extensions>
			<code_completion />
			<envvars />
			<debugger />
			<lib_finder disable_auto="1" />
		</Extensions>
	</Project>
</CodeBlocks_project_file>
