<?xml version="1.0" encoding="UTF-8" standalone="yes" ?>
<CodeBlocks_project_file>
	<FileVersion major="1" minor="6" />
	<Project>
		<Option title="cktool" />
		<Option pch_mode="2" />
		<Option compiler="gcc" />
		<Build>
			<Target title="Debug">
				<Option output="bin/Debug/cktool" prefix_auto="1" extension_auto="1" />
				<Option object_output="obj/Debug/" />
				<Option external_deps="../cklib/bin/Debug/libcklib.a;" />
				<Option type="1" />
				<Option compiler="gcc" />
				<Option parameters="--pds 172.24.22.51/1 --pds 172.24.22.51/2r --verbose rotate 1,1,1" />
				<Compiler>
					<Add option="-g" />
				</Compiler>
			</Target>
			<Target title="Release">
				<Option output="bin/Release/cktool" prefix_auto="1" extension_auto="1" />
				<Option object_output="obj/Release/" />
				<Option external_deps="../cklib/bin/Release/libcklib.a;" />
				<Option type="1" />
				<Option compiler="gcc" />
				<Compiler>
					<Add option="-O2" />
				</Compiler>
				<Linker>
					<Add option="-s" />
				</Linker>
			</Target>
		</Build>
		<Compiler>
			<Add option="-Wall" />
			<Add option="-fexceptions" />
			<Add directory="../cklib" />
		</Compiler>
		<Linker>
			<Add option="../cklib/bin/$(TARGET_NAME)/libcklib.a -static-libgcc -static-libstdc++" />
		</Linker>
		<ExtraCommands>
			<Add after="cp bin/$(TARGET_NAME)/$(TARGET_OUTPUT_BASENAME).exe ../bin/$(TARGET_OUTPUT_BASENAME).exe" />
		</ExtraCommands>
		<Unit filename="cktool.cpp" />
		<Extensions>
			<code_completion />
			<envvars />
			<debugger />
			<DoxyBlocks>
				<comment_style block="0" line="0" />
				<doxyfile_project />
				<doxyfile_build />
				<doxyfile_warnings />
				<doxyfile_output />
				<doxyfile_dot />
				<general />
			</DoxyBlocks>
		</Extensions>
	</Project>
</CodeBlocks_project_file>
